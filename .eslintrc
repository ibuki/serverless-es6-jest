{
  "root": true,
  "env": {
    "node": true,
    "browser": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "google"
  ],
  "plugins": [
    "jest"
  ],
  "rules": {
    "no-console": "off",
    "object-curly-spacing": [2, "always"]
  }
}